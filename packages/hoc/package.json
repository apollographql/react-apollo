{
  "name": "@apollo/react-hoc",
  "description": "React Apollo `graphql` higher-order component.",
  "version": "0.1.0-beta.4",
  "author": "opensource@apollographql.com",
  "keywords": [
    "apollo",
    "graphql",
    "react",
    "hoc"
  ],
  "license": "MIT",
  "main": "./lib/react-hoc.cjs.js",
  "module": "./lib/react-hoc.esm.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "apollographql/react-apollo"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -Rf ./node_modules ./lib/* ./meta/bundlesize/* ./meta/coverage/*",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "../../node_modules/.bin/tsc -p ./config",
    "postbuild": "../../node_modules/.bin/rollup -c ./config/rollup.config.js",
    "watch": "node ../../node_modules/tsc-watch/lib/tsc-watch.js --onSuccess \"npm run postbuild\" -p ./config",
    "predeploy": "npm run build",
    "deploy": "npm publish --tag beta",
    "test": "../../node_modules/jest/bin/jest.js --config ../../config/jest.config.js --testPathPattern packages/hoc",
    "test:watch": "../../node_modules/jest/bin/jest.js --config ../../config/jest.config.js --testPathPattern packages/hoc --watch",
    "test:cjs": "npm run build && ../../node_modules/jest/bin/jest.js --config ../../config/jest.cjs.config.js --testPathPattern packages/hoc",
    "test:umd": "npm run build && ../../node_modules/jest/bin/jest.js --config ../../config/jest.umd.config.js --testPathPattern packages/hoc"
  },
  "peerDependencies": {
    "apollo-client": "^2.6.0",
    "graphql": "^14.2.1",
    "react": "^16.8.6"
  },
  "dependencies": {
    "@apollo/react-common": "file:../common",
    "@apollo/react-components": "file:../components",
    "ts-invariant": "^0.4.2",
    "tslib": "^1.9.3"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  }
}
