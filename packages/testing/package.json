{
  "name": "@apollo/react-testing",
  "description": "React Apollo testing utilities.",
  "version": "0.1.0-beta.2",
  "author": "opensource@apollographql.com",
  "keywords": [
    "apollo",
    "graphql",
    "react",
    "testing"
  ],
  "license": "MIT",
  "main": "./lib/react-testing.cjs.js",
  "module": "./lib/react-testing.esm.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "apollographql/react-apollo"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -Rf ./node_modules ./lib/* ./meta/coverage/*",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "../../node_modules/.bin/tsc -p ./config",
    "postbuild": "../../node_modules/.bin/rollup -c ./config/rollup.config.js",
    "watch": "node ../../node_modules/tsc-watch/lib/tsc-watch.js --onSuccess \"npm run postbuild\" -p ./config",
    "predeploy": "npm run build",
    "deploy": "npm publish --tag beta",
    "test": "../../node_modules/.bin/jest --config ./config/jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:full": "npm run type-check && npm run test -- --coverage"
  },
  "peerDependencies": {
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "apollo-utilities": "^1.3.0",
    "graphql": "^14.2.1",
    "react": "^16.8.6"
  },
  "dependencies": {
    "@apollo/react-common": "file:../common",
    "tslib": "^1.10.0"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  }
}
