{
  "name": "typescript-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-components": "beta",
    "apollo": "^2.15.0",
    "apollo-cache-inmemory": "latest",
    "apollo-client": "latest",
    "apollo-link-http": "latest",
    "graphql-tag": "^2.10.1",
    "react": "../../node_modules/react",
    "react-dom": "../../node_modules/react-dom"
  },
  "devDependencies": {
    "@types/graphql": "14.2.2",
    "@types/jest": "24.0.14",
    "@types/node": "12.6.2",
    "@types/prop-types": "15.7.1",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-test-renderer": "16.8.2",
    "react-scripts-ts": "3.1.0",
    "react-test-renderer": "16.8.6",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "schema": "apollo client:download-schema --endpoint=https://uyx9x.sse.codesandbox.io ./src/schema.json",
    "types": "apollo client:codegen ./src/__generated__/types.ts --outputFlat --includes=./src/**/queries.ts --addTypename --localSchemaFile=./src/schema.json --target=typescript"
  }
}
